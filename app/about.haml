    %section.team-about-slider
      .container
        .content-title
          .team-about-content.wow.animated.fadeInRight{ 'data-wow-duration' => '1500ms', 'data-wow-delay' => '.2s'  }
            .team-about-title
              %h1 Nuestro gran 
